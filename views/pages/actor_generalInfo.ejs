<%- include('../partials/header') %> 

<main class="main text-center" id="main">
  <h1 class="text-center">General Information About the Program This Actor Performed In</h1>
  <% data.forEach(item => {%>
    <p><%= item.fName %> <%= item.lName %></p>
    <p><%= item.title %></p>
    <p><%= item.yr_released %></p>
    <p><%= item.rating %></p>
    <p><%= item.showing %></p>
    <p><%= item.runtime %></p>
  <% }) %>  
</main>

<%- include('../partials/footer') %>