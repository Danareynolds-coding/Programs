<%- include('../partials/header') %> 
<main class="main" id="main">
  <div class="container mt-4">
        <div class="row text-center">
            <div class="col">
                <div class="card">
                    <% data.forEach(item => { %>
                    <div class="card-body">
                        <p><%= item.genre %> 
                        <p><%= item.title %></p>
                        <p><%= item.yr_released %></p>
                        <p><%= item.rating %></p>
                    </div>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>

</main>

<%- include('../partials/footer') %> 